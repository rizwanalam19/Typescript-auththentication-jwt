{{>header}}
<table class="table table-bordered">
  <thead>
    <tr>
      <th scope="col">ID </th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Password</th>
      <th scope="col">Tokens</th>

    </tr>
  </thead>
  <tbody>
        {{#each users}}
    <tr>
      <th scope="row">{{ID}}</th>
      <td>{{FIRSTNAME}}</td>
      <td>{{LASTNAME}}</td>
      <td>{{EMAIL}}</td>
      <td>{{PASSWORD}}</td>
      <td>{{TOKENS}}</td>

    </tr>
  {{/each}}
  </tbody>
</table>

{{>footer}}